! Hide the scroll bar
URxvt.scrollBar:false

! URxvt.font: -benis-uushi-medium-R-Normal--11-90-75-75-P-58-ISO10646-1
! URxvt.font: -lucy-tewi-medium-r-normal--11-90-100-100-c-60-ISO10646-1
! URxvt.font: xft:tewi:pixelsize=8
URxvt.font: xft:DejaVu Sans Mono:size=11

! Dont use a bold font
URxvt.boldFont:

! Define the padding
URxvt.internalBorder: 10
URxvt.externalBorder: 0

! Make Clickable Links
URxvt.perl-ext-common: default,matcher
URxvt*matcher.button: 1
URxvt.url-launcher: /usr/bin/firefox-beta
URxvt.colorUL: #7086b2

! Turn off beep
URxvt*vbell: True

! Colors
*.foreground: #839496
*.background: #09090d
*.cursorColor: #839496

! black
*.color0: #353540
*.color8: #545466
! red
*.color1: #8c5760
*.color9: #b26f7a
! green
*.color2: #7b8c58
*.color10: #9db270
! yellow
*.color3: #8c6e43
*.color11: #b28c55
! blue
*.color4: #58698c
*.color12: #7086b2
! magenta
*.color5: #7b5e7d
*.color13: #9c77b2
! cyan
*.color6: #66808c
*.color14: #82a2b2
! white
*.color7: #8c8b8b
*.color15: #b8b8c8
