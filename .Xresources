!Remove the scroll bar
URxvt.scrollBar: false

! The font
URxvt*font: -lucy-tewi-medium-r-normal--11-90-100-100-c-60-ISO10646-1

! Dont use bold fonts they look stupid
URxvt.boldFont:

! Define the padding
URxvt.internalBorder: 10
URxvt.externalBorder: 0

! Make clickable links
URxvt.perl-ext-common: default,matcher
URxvt*matcher.button: 1
URxvt.url-launcher: /usr/bin/firefox
URxvt.colorUL: #81a2be

! Colors
*.foreground:	#c5c8c6
*.background:	#1d1f21
*.cursorColor:	#c5c8c6

! Black
*.color0:	#282a2e
*.color8:	#373b41

! Red
*.color1:	#a54242
*.color9:	#cc6666

! Green
*.color2:	#8c9440
*.color10:	#b5bd68

! Yellow
*.color3:	#de935f
*.color11:	#f0c674

! Blue
*.color4:	#6287a6
*.color12:	#729fc4

! Magenta
*.color5:	#6c467a
*.color13:	#9f5db8

! Cyan
*.color6:	#467a6a
*.color14:	#60ac94

! White
