! Remove the scroll bar
URxvt.scrollBar: false

! The font
URxvt*font: xft:Tewi:pixelsize=11:antialias=false

! Don't use this font pref. it's shit
!URxvt.font: xft:tewi-8

! Use a empty string for the bold font
URxvt.boldFont:

! Define the padding
URxvt.internalBorder: 10
URxvt.externalBorder: 0

! Made using http://terminal.sexy
! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #a54242
*.color9:       #cc6666

! green
*.color2:       #8c9440
*.color10:      #b5bd68

! yellow
*.color3:       #de935f
*.color11:      #f0c674

! blue
*.color4:       #5f819d
*.color12:      #81a2be

! magenta
*.color5:       #85678f
*.color13:      #b294bb

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #707880
*.color15:      #c5c8c6


! Make clickable links
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher:	/usr/bin/firefox-aurora
URxvt.matcher.button:	1
URxvt.colorUL: #81a2be
